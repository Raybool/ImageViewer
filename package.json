{
  "name": "image-viewer",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "start": "react-native start",
    "test": "jest",
    "watch:ios": "react-native run-ios --simulator=\"iPhone 14\"",
    "start:clear": "react-native start --reset-cache",
    "prettier": "prettier --check ./",
    "clean:ios": "xcrun simctl erase all && watchman watch-del-all && rm -rf node_modules && yarn && cd ios/ && rm -rf Pods && pod install && cd ..",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "axios": "^1.7.7",
    "babel-plugin-module-resolver": "^5.0.2",
    "mobx": "^6.13.3",
    "mobx-react": "^9.1.1",
    "react": "18.3.1",
    "react-native": "0.75.3",
    "react-native-gesture-handler": "^2.20.0",
    "react-native-reanimated": "^3.15.4",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-screens": "^3.34.0",
    "react-native-svg": "^15.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.25.7",
    "@babel/plugin-transform-private-methods": "^7.25.7",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.3",
    "@react-native/eslint-config": "0.75.3",
    "@react-native/metro-config": "0.75.3",
    "@react-native/typescript-config": "0.75.3",
    "@types/jest": "^29.5.13",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": "18.*",
    "yarn": "1.22.*"
  }
}
